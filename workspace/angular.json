{
	"version": 1,
	"cli": {
		"defaultCollection": "@nrwl/angular"
	},
	"defaultProject": "host",
	"schematics": {
		"@nrwl/angular": {
			"application": {
				"linter": "eslint"
			},
			"library": {
				"linter": "eslint"
			},
			"storybook-configuration": {
				"linter": "eslint"
			}
		},
		"@nrwl/angular:application": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest",
			"e2eTestRunner": "cypress"
		},
		"@nrwl/angular:library": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest"
		},
		"@nrwl/angular:component": {
			"style": "scss"
		}
	},
	"projects": {
		"host": {
			"projectType": "application",
			"root": "apps/host",
			"sourceRoot": "apps/host/src",
			"prefix": "workspace",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/apps/host",
						"index": "apps/host/src/index.html",
						"main": "apps/host/src/main.ts",
						"polyfills": "apps/host/src/polyfills.ts",
						"tsConfig": "apps/host/tsconfig.app.json",
						"aot": true,
						"assets": [
							"apps/host/src/favicon.ico",
							"apps/host/src/assets"
						],
						"styles": [
							"apps/host/src/styles.scss"
						],
						"scripts": [],
						"extraWebpackConfig": "apps/host/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "apps/host/src/environments/environment.ts",
									"with": "apps/host/src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							],
							"extraWebpackConfig": "apps/host/webpack.prod.config.js"
						}
					}
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"options": {
						"browserTarget": "host:build",
						"port": 4200,
						"publicHost": "http://localhost:4200",
						"extraWebpackConfig": "apps/host/webpack.config.js"
					},
					"configurations": {
						"production": {
							"browserTarget": "host:build:production",
							"extraWebpackConfig": "apps/host/webpack.prod.config.js"
						}
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "host:build",
						"extraWebpackConfig": "apps/host/webpack.config.js"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"apps/host/src/**/*.ts",
							"apps/host/src/**/*.html"
						]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": [
						"coverage/apps/host"
					],
					"options": {
						"jestConfig": "apps/host/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		},
		"host-e2e": {
			"root": "apps/host-e2e",
			"sourceRoot": "apps/host-e2e/src",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/host-e2e/cypress.json",
						"tsConfig": "apps/host-e2e/tsconfig.e2e.json",
						"devServerTarget": "host:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "host:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"apps/host-e2e/**/*.{js,ts}"
						]
					}
				}
			}
		},
		"footer": {
			"projectType": "application",
			"root": "apps/footer",
			"sourceRoot": "apps/footer/src",
			"prefix": "workspace",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/apps/footer",
						"index": "apps/footer/src/index.html",
						"main": "apps/footer/src/main.ts",
						"polyfills": "apps/footer/src/polyfills.ts",
						"tsConfig": "apps/footer/tsconfig.app.json",
						"aot": true,
						"assets": [
							"apps/footer/src/favicon.ico",
							"apps/footer/src/assets"
						],
						"styles": [
							"apps/footer/src/styles.scss"
						],
						"scripts": [],
						"extraWebpackConfig": "apps/footer/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "apps/footer/src/environments/environment.ts",
									"with": "apps/footer/src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							],
							"extraWebpackConfig": "apps/footer/webpack.prod.config.js"
						}
					}
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"options": {
						"browserTarget": "footer:build",
						"port": 4201,
						"publicHost": "http://localhost:4201",
						"extraWebpackConfig": "apps/footer/webpack.config.js"
					},
					"configurations": {
						"production": {
							"browserTarget": "footer:build:production",
							"extraWebpackConfig": "apps/footer/webpack.prod.config.js"
						}
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "footer:build",
						"extraWebpackConfig": "apps/footer/webpack.config.js"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"apps/footer/src/**/*.ts",
							"apps/footer/src/**/*.html"
						]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": [
						"coverage/apps/footer"
					],
					"options": {
						"jestConfig": "apps/footer/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		},
		"footer-e2e": {
			"root": "apps/footer-e2e",
			"sourceRoot": "apps/footer-e2e/src",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/footer-e2e/cypress.json",
						"tsConfig": "apps/footer-e2e/tsconfig.e2e.json",
						"devServerTarget": "footer:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "footer:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"apps/footer-e2e/**/*.{js,ts}"
						]
					}
				}
			}
		}
	}
}